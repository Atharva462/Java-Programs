<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
<head>
    <title>Sum Calculator</title>
</head>
<body>
    <%
        int a = 15;
        int b = 25;
        int sum = a + b;
    %>
    <h2>Sum of <%= a %> and <%= b %> is: <%= sum %></h2>
</body>
</html>
